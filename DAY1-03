data <- c(200, 300, 400, 600, 1000)

min_value <- min(data)
max_value <- max(data)

normalized_data_min_max <- (data - min_value) / (max_value - min_value)

cat("Min-Max Normalization (0 to 1):\n", normalized_data_min_max, "\n")

mean_value <- mean(data)
standard_deviation <- sd(data)

normalized_data_zscore <- (data - mean_value) / standard_deviation

cat("Z-Score Normalization:\n", normalized_data_zscore)
